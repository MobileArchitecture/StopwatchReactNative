{
  "name": "stopwatch-react-native",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "eslint": "^4.10.0",
    "eslint-plugin-flow": "^2.29.1",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-react-native": "^3.1.0",
    "jest-expo": "^22.0.0",
    "jest-json-to-tap": "^2.1.0",
    "react-native-scripts": "1.7.0",
    "react-test-renderer": "16.0.0-beta.5",
    "standard": "^10.0.3"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "coverage": "node node_modules/jest/bin/jest.js",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "tdd": "node node_modules/jest/bin/jest.js --json 2>/dev/null | jest-json-to-tap"
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ]
  },
  "dependencies": {
    "expo": "^22.0.2",
    "lodash": "^4.17.4",
    "react": "16.0.0-beta.5",
    "react-native": "^0.49.5",
    "react-native-button": "^2.1.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
